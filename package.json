    "postcss-nested": "^1.0.0",
    "precss": "^1.4.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "shelljs": "^0.7.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-s3-plugin": "^0.9.2"
  },
  "dependencies": {
    "config": "^1.24.0",
    "immutable": "^3.8.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
