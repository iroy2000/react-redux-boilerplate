  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "automock": true,
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx|json)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "keywords": [
    "React",
    "Redux",
    "webpack"
  ]
